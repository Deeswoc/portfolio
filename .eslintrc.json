{
    "env": {
        "browser": true

    },
    // "extends": [
    //     "plugin:react/recommended",
    //     "plugin:@next/next/recommended",
    //     "google"
    // ],
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "max-len": [
            "error",
            {
                "ignoreTemplateLiterals": true,
                "ignoreStrings": true
            }
        ]
    }
}